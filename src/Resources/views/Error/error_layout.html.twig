{% spaceless %}
<!DOCTYPE html>
<html>
<head>
    {% block head %}
    <meta charset="utf-8">
    <title>{% block title %}{{ statusCode }} - ERROR{% endblock %}</title>
    <meta name="generator" content="Contao Open Source CMS">
    <meta name="viewport" content="width=device-width,initial-scale=1.0">
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,600,700,800,300' rel='stylesheet' type='text/css'>
    <style>
        {% include '@ContaoCore/error.css.twig' %}
    </style>
    {% endblock %}
</head>
<body>
<div id="header">
    <div class="wrap">
        <div class="header-logo">
            <img width="196" height="65" title="" src="data:image/png;base64," alt="Contao Open source CMS"/>
        </div>

        <div class="search">
            <!-- FIXME: hardcoded to German search here. -->
            <form method="get" action="http://contao.org/de/search.html" target="_blank">
                <div class="form-row">
                    <label for="search-id">
                        <img src="data:image/png;base64," alt="Search on Contao website">
                    </label>

                    <input name="keywords" id="search-id" type="search" placeholder="Search on Contao website">

                    <button type="submit" class="sf-button">
                                    <span class="border-l">
                                        <span class="border-r">
                                            <span class="btn-bg">OK</span>
                                        </span>
                                    </span>
                    </button>
                </div>
            </form>
        </div>
    </div>
</div>
<div id="content">
    <div class="wrap">
        <div class="error">
            {% block message %}<h1>{{ block('title') }}</h1>{% endblock %}
            <div class="block-error">
                <div class="inner">
                    <div class="text-error">
                        <div class="open-quote"></div>
                        {% block main %}
                            {% set _block = block('matter') %}
                            {% if _block is not empty %}
                                <h2>{{ error.matter }}</h2>
                                {{ _block|raw }}
                            {% endif %}
                            {% set _block = block('howToFix') %}
                            {% if _block is not empty %}
                                <h2>{{ error.howToFix }}</h2>
                                {{ _block|raw }}
                            {% endif %}
                            {% set _block = block('explain') %}
                            {% if _block is not empty %}
                                <h3>{{ error.more }}</h3>
                                {{ _block|raw }}
                            {% endif %}
                        {% endblock %}
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div id="footer">
    <div class="wrap">
        {% block hint %}{{ error.hint|format(template)|raw }}{% endblock %}
    </div>
</div>
</body>
</html>
{% endspaceless %}
