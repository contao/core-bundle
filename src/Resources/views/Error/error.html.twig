{% trans_default_domain 'contao_exception' %}
{% extends "@ContaoCore/Error/layout.html.twig" %}
{% block title %}
    {{ 'XPT.error'|trans }}
{% endblock %}
{% block matter %}
    <p>{{ 'XPT.errorOccurred'|trans }}</p>
{% endblock %}
{% block howToFix %}
