{% spaceless %}
<!DOCTYPE html>
<html>
<head>
    {% block head %}
        <meta charset="utf-8">
        <title>{% block title %}{{ statusCode }} Error{% endblock %}</title>
        <meta name="generator" content="Contao Open Source CMS">
        <meta name="viewport" content="width=device-width,initial-scale=1.0,shrink-to-fit=no">
        <link rel="stylesheet" href="//fonts.googleapis.com/css?family=Open+Sans:300">
        {% block style %}
            <style>
                html {
                    background: #fff;
                }
                body {
                    margin: 0;
                    padding: 0;
                    font: 16px "Open Sans", Helvetica, sans-serif;
                    font-weight: 300;
                    color: #333;
                }
                a, a:visited {
                    text-decoration: none;
                    color: #e4790f;
                }
                a:hover {
                    text-decoration: underline;
                }
                h1, h2, h3 {
                    font-weight: 300;
                }
                h1 {
                    margin: 0;
                    padding-bottom: 2%;
                    font-size: 3em;
                }
                h2, h3 {
                    font-size: 1.5em;
                }
                #header {
                    padding: 12px 0 10px;
                }
                #content {
                    padding: 3% 0;
                    background-color: #eee;
                }
                #footer {
                    font-size: 0.75em;
                    text-align: center;
                    padding: .5em 0;
                    color: #999;
                }
                .wrap {
                    width: 60%;
                    margin: 0 auto;
                    padding: 1em;
                    overflow: hidden;
                }
                .header-logo {
                    display: inline-block;
                }
                .block-error {
                    margin-bottom: 20px;
                    background-color: #fff;
                    border: 1px solid #dfdfdf;
                    border-radius: 16px;
                    padding: 15px 30px 24px;
                    word-wrap: break-word;
                    overflow: hidden;
                }
                .block-error:before {
                    float: left;
                    content: "</";
                    font-size: 5em;
                    color: #ccc;
                }
                .block-error .inner {
                    padding-left: 13%;
                }
            </style>
       {% endblock %}
    {% endblock %}
</head>
<body>
    <div id="header">
        <div class="wrap">
            <div class="header-logo">
                <img width="200" height="54" src="data:image/svg+xml;base64," alt="Contao Open source CMS"/>
            </div>
        </div>
    </div>
    <div id="content">
        <div class="wrap">
            <div class="error">
                {% block message %}<h1>{{ block('title') }}</h1>{% endblock %}
                <div class="block-error">
                    <div class="inner">
                        <div class="text-error">
                            {% block main %}
                                {% set _block = block('matter') %}
                                {% if _block|trim is not empty %}
                                    <h2>{{ error.matter|default("What's the matter?") }}</h2>
                                    {{ _block|raw }}
                                {% endif %}
                                {% set _block = block('howToFix') %}
                                {% if _block|trim is not empty %}
                                    <h2>{{ error.howToFix|default('How can I fix the issue?') }}</h2>
                                    {{ _block|raw }}
                                {% endif %}
                                {% set _block = block('explain') %}
                                {% if _block|trim is not empty %}
                                    <h3>{{ error.more|default('Tell me more, please') }}</h3>
                                    {{ _block|raw }}
                                {% endif %}
                            {% endblock %}
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div id="footer">
        <div class="wrap">
            {% block hint %}{{ error.hint|default('To customize this notice, create a custom Twig template overriding @ContaoCore/Error/error.html.twig.')|format(template)|raw }}{% endblock %}
        </div>
    </div>
</body>
</html>
{% endspaceless %}
