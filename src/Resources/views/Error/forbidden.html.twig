{% extends "@ContaoCore/Error/error_layout.html.twig" %}
{% block title %}
    {{ statusCode }} - {{ error.forbidden }}
{% endblock %}
{% block matter %}
    <p>{{ error.notAuthenticated }}</p>
{% endblock %}
{% block howToFix %}
    <p>{{ error.forbiddenFix|format(base, adminEmail)|raw }}</p>
{% endblock %}
