{% extends "@ContaoCore/Error/error_layout.html.twig" %}
{% block title %}
    {{ statusCode }} - {{ error.incomplete }}
{% endblock %}
{% block matter %}
    <p>{{ error.incompleteInstallation }}</p>
{% endblock %}
{% block howToFix %}
    <p>{{ error.incompleteFix|raw }}</p>
{% endblock %}
{% block explain %}
    <p>{{ error.incompleteExplain|raw }}</p>
{% endblock %}
