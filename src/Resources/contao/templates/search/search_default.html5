  <h3><a href="<?= $this->href ?>" title="<?= $this->title ?>"><?= $this->link ?></a></h3>

  <?php if ($this->context): ?>
    <p class="context"><?= $this->context ?></p>
  <?php endif; ?>

  <p class="url"><?= $this->url ?></p>

</div>
