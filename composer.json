        "contao-components/dropzone": "^5.0.1",
        "contao-components/highlight": "^9.0",
        "contao-components/html5shiv": "^3.7.3",
        "contao-components/jquery": "^1.12 || ^2.0 || ^3.0",
        "contao-components/jquery-ui": "^1.11.4",
