{
    "name": "contao/core-bundle",
    "type": "symfony-bundle",
    "description": "Contao 4 core bundle",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Leo Feyer",
            "homepage": "https://github.com/leofeyer"
        }
    ],
    "require": {
        "php": "^7.1",
