        <source>Edit the theme settings</source>
        <target>Уреди подешавања теме</target>
      </trans-unit>
      <trans-unit id="tl_theme.new.0">
        <source>New</source>
        <target>Ново</target>
      </trans-unit>
      <trans-unit id="tl_theme.new.1">
        <source>Create a new theme</source>
        <target>Направи нову тему</target>
      </trans-unit>
