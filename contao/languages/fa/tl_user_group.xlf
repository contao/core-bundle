        <target>گروه‌های اعضاء مجاز</target>
      </trans-unit>
      <trans-unit id="tl_user_group.alexf_legend">
        <source>Allowed fields</source>
        <target>فیلدهای مجاز</target>
      </trans-unit>
      <trans-unit id="tl_user_group.account_legend">
        <source>Group settings</source>
        <target>تنظیمات گروه</target>
      </trans-unit>
      <trans-unit id="tl_user_group.new.0">
        <source>New</source>
        <target>جدید</target>
      </trans-unit>
      <trans-unit id="tl_user_group.new.1">
        <source>Create a new user group</source>
        <target>ایجاد یک گروه کاربری جدید</target>
      </trans-unit>
      <trans-unit id="tl_user_group.show">
        <source>Show the details of user group ID %s</source>
        <target>نمایش جزئیات گروه کاربری شناسه %s</target>
      </trans-unit>
      <trans-unit id="tl_user_group.edit">
        <source>Edit user group ID %s</source>
        <target>ویرایش گروه کاربری شناسه %s</target>
      </trans-unit>
      <trans-unit id="tl_user_group.copy">
        <source>Duplicate user group ID %s</source>
        <target>تکثیر گروه کاربری شناسه %s</target>
      </trans-unit>
      <trans-unit id="tl_user_group.delete">
        <source>Delete user group ID %s</source>
        <target>حذف گروه کاربری شناسه %s</target>
      </trans-unit>
      <trans-unit id="tl_user_group.toggle.0">
        <source>Activate/deactivate user group</source>
        <target>فعال/غیرفعال کردن گروه کاربری</target>
      </trans-unit>
      <trans-unit id="tl_user_group.toggle.1">
        <source>Deactivate user group ID %s</source>
        <target>غیرفعال کردن گروه کاربری شناسه‌ی %s</target>
      </trans-unit>
      <trans-unit id="tl_user_group.toggle.2">
        <source>Activate user group ID %s</source>
        <target>فعال کردن گروه کاربری شناسه‌ی %s</target>
      </trans-unit>
